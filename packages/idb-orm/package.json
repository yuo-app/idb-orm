{
  "name": "idb-orm",
  "type": "module",
  "version": "0.0.0",
  "description": "A lightweight, type-safe ORM for IndexedDB that closely matches the supabase-js API.",
  "author": "Rettend",
  "license": "MIT",
  "homepage": "https://github.com/yuo-app/idb-orm",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuo-app/idb-orm.git"
  },
  "bugs": {
    "url": "https://github.com/yuo-app/idb-orm/issues"
  },
  "keywords": [
    "indexeddb",
    "idb",
    "orm",
    "supabase",
    "supabase-js"
  ],
  "module": "index.ts",
  "files": [
    "src.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest --coverage",
    "test:ui": "vitest --coverage --ui",
    "release": "bumpp && bun run build && bun publish"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/bun": "latest",
    "bumpp": "^9.8.1",
    "eslint": "^9.14.0",
    "fake-indexeddb": "^6.0.0",
    "jsdom": "^25.0.1",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.5",
    "vitest-indexeddb": "^0.0.0"
  }
}
